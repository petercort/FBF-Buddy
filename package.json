{
    "name": "fbf-buddy",
    "private": true,
    "version": "0.0.0",
    "description": "Cycling event app for Discord - Microservices Monorepo",
    "type": "module",
    "engines": {
        "node": ">=20.x"
    },
    "scripts": {
        "start": "docker-compose up",
        "start:bot": "cd services/discord-bot && npm start",
        "start:backend": "cd services/backend && npm start",
        "register": "cd services/discord-bot && npm run register",
        "dev:bot": "cd services/discord-bot && npm run dev",
        "dev:backend": "cd services/backend && npm run dev",
        "lint": "npm run lint:bot && npm run lint:backend && npm run lint:root",
        "lint:bot": "cd services/discord-bot && npm run lint",
        "lint:backend": "cd services/backend && npm run lint",
        "lint:root": "eslint src/**/*.js",
        "install:all": "npm install && cd services/discord-bot && npm install && cd ../backend && npm install"
    },
    "author": "Peter Cort",
    "license": "MIT",
    "dependencies": {
        "@azure/identity": "^4.13.0",
        "@azure/keyvault-secrets": "^4.9.0",
        "@npmcli/fs": "^4.0.0",
        "axios": "^1.12.0",
        "clean": "^4.0.2",
        "discord.js": "^14.17.3",
        "dotenv": "^17.2.3",
        "express": "^5.1.0",
        "fbf-buddy": "file:",
        "pg": "^8.11.1",
        "sequelize": "^6.37.7"
    },
    "devDependencies": {
        "@eslint/js": "^9.17.0",
        "eslint": "^9.18.0",
        "globals": "^16.0.0",
        "jest": "^30.2.0",
        "nodemon": "^3.1.9"
    },
    "overrides": {
        "validator": {
            ".": "13.15.15",
            "comment": "CVE-2025-56200: Known vulnerability in validator.js <=13.15.15. Update to 13.15.16+ when available. See SECURITY.md for details."
        }
    }
}
