{
    "name": "fbf-buddy",
    "private": true,
    "version": "1.5.11",
    "description": "Cycling event app for Discord - Microservices Monorepo",
    "type": "module",
    "engines": {
        "node": ">=24.x"
    },
    "scripts": {
        "start": "docker-compose up",
        "start:bot": "cd services/discord-bot && npm start",
        "start:backend": "cd services/backend && npm start",
        "register": "cd services/discord-bot && npm run register",
        "dev:bot": "cd services/discord-bot && npm run dev",
        "dev:backend": "cd services/backend && npm run dev",
        "test": "NODE_OPTIONS=--experimental-vm-modules jest",
        "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch",
        "test:coverage": "NODE_OPTIONS=--experimental-vm-modules jest --coverage",
        "lint": "npm run lint:bot && npm run lint:backend",
        "lint:bot": "cd services/discord-bot && npm run lint",
        "lint:backend": "cd services/backend && npm run lint",
        "install:all": "npm install && cd services/discord-bot && npm install && cd ../backend && npm install"
    },
    "author": "Peter Cort",
    "license": "MIT",
    "dependencies": {
        "@azure/identity": "^4.13.0",
        "@azure/keyvault-secrets": "^4.9.0",
        "@npmcli/fs": "^4.0.0",
        "axios": "^1.12.0",
        "clean": "^4.0.2",
        "discord.js": "^14.24.0",
        "dotenv": "^17.2.3",
        "express": "^5.1.0",
        "fbf-buddy": "file:",
        "pg": "^8.11.1",
        "sequelize": "^6.37.7"
    },
    "devDependencies": {
        "@eslint/js": "^9.17.0",
        "eslint": "^9.18.0",
        "globals": "^16.0.0",
        "jest": "^30.2.0",
        "nodemon": "^3.1.9"
    }
}
